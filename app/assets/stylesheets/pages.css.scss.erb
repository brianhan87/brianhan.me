/* Universal Styles ----------- */
body {
  margin: 0px;
  margin-top: -16px;
  font-family: "HelveticaNeue", Sans-Serif;
  font-weight: bold;
}

hr {
  border: 0px;
  height: 1px;
  background-color: black;
}

h1 {
  margin: 0;
  padding: 0;
  font-size: 7em;
  letter-spacing: 0.25em;
  font-style: italic;
  color: yellow;
  opacity: 0.9;
  font-family: "Futura", Sans-serif;
  text-transform: uppercase;
  padding-top: 1em;
  &.brianhan {
    font-size: 4.375em;
    color: black;
    font-family: "HelveticaNeue", Sans-serif;
    font-style: normal;
    letter-spacing: 0;
    padding-top: 0;
  }
}

h2 {
  margin: 0; 
  padding: 0;
  font-weight: normal;
  font-style: italic;
  font-family: "TimesNewRoman", Serif;
  font-size: 20px;
}

p {
  font-size: 1.7em;
  font-weight: normal;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  color: white;
  font-family: "Futura", Sans-serif;
}

.container {
  float: left;
  -webkit-transition: 0.2s;
  -moz-transition: 0.2s;
  -ms-transition: 0.2s;
  -o-transition: 0.2s;
  transition: 0.2s;
}

#home {
  background-image: url(<%= asset_path 'cali_beach_light_1920.jpg' %>);
  background-size: 100%, 100%;
  width: 100%;
  height: 66em;
}

.home {
  float: left;
  background-color: white;
  margin-top: 19.625em;
  margin-left: 28.33%;
  width: 33.33%;
  text-align: center;
  color: black;
  padding: 5%;
  opacity: 0.85;
}

#hello {
  float: left;
  width: 100%;
  
  #hello-koala {
    float: left;
    background-image: url(<%= asset_path 'hello_koala_1000_darkpixel.jpg' %>);
    background-size: 115%;
    width: 100%;
    height: 66em;
    

    .koala {
      height: 100%;
      padding-left: 5%;
      padding-right: 5%;
      text-align: left;
      background-color: transparent;
      opacity: 1;

      &:hover {
        // background-color: black;
        background-color: hsla(165, 74%, 14%, 0.93);
        background-color: rgba(9, 62, 49, 0.93);
        background-color: #093e31;
        padding-right: 10%;
        opacity: 0.7;
        -webkit-transition: 0.2s;
        -moz-transition: 0.2s;
        -ms-transition: 0.2s;
        -o-transition: 0.2s;
        transition: 0.2s;
      }
    }
  }
}

#skills {
  float: left;
  width: 100%;

  #my-skills {
    float: left;
    background-image: url(<%= asset_path 'workstation_1200.jpg' %>);
    background-size: 100%;
    width: 100%;
    height: 66em;

    .skills {
      float: left;
      width: 50%;
      text-align: left;
      background-color: transparent;
      height: 100%;
      padding-left: 5%;
      padding-right: 5%;
      &:hover {
        background-color: hsla(195, 74%, 14%, 0.93);
        // background-color: hsla(174, 47%, 14%, 0.93);
        // background-color: #133431;
        opacity: 0.7;
        padding-right: 10%;
        -webkit-transition: 0.2s;
        -moz-transition: 0.2s;
        -ms-transition: 0.2s;
        -o-transition: 0.2s;
        transition: 0.2s;
      }

      ul {
        float: left;
        padding-left: 0;
        li { 
          float: left;
          list-style-type: none;
          text-align: left;
          margin-right: 1.5em; 
          margin-top: 1.5em; 
          margin-bottom: 1.5em; 
        } 
      }

      b {
        padding: 0.5em;
        border-radius: 5px;
        opacity: 0.9;
        font-size: 1.5em;
        text-transform: uppercase;
        font-family: "Futura", Sans-serif;
      }
    }
  }
}


.htmlcss {
  background-color: hsla(35, 100%, 67%, 0.3);
  background-color: #ffb957;
}
.photoshop {
  background-color: hsla(214, 79%, 64%, 0.82);
  background-color: #5b9aec;
}
.js {
  background-color: hsla(58, 79%, 64%, 0.82);
  background-color: #ece75b;
}
.ror {
  background-color: hsla(359, 79%, 64%, 0.82);
  background-color: #ec5b5d;
}
.agile {
  background-color: hsla(140, 79%, 64%, 0.82);
  background-color: #5bec8b;
}
.git {
  background-color: turquoise;
}
.video {
  background-color: #ccc;
}

/* Small Desktop - 1280px to 1680px width ----------- */
@media only screen 
and (min-width: 80em)
and (max-width : 105em) {
  #home {
    height: 58em;
  }
  
  #hello-koala,
  #skills {
    height: 30em;
  }


  .koala p {
    font-size: 1.2em;
  }


}

